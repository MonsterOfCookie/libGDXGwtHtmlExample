<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to="html">
    <inherits name='com.supercookie.example.GdxDefinition' />
    <replace-with class="com.badlogic.gdx.backends.gwt.SuperDevModeIndicatorTrue">
        <when-type-is class="com.badlogic.gdx.backends.gwt.SuperDevModeIndicator"/>
        <when-property-is name="superdevmode" value="on" />
    </replace-with>

    <set-configuration-property name="user.home" value="" />
</module>
